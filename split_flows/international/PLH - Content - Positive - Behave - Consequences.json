      "uuid": "d6120275-c4b0-42d6-b338-df84e7bd034e"
    },
    {
      "actions": [
        {
          "attachments": [],
          "quick_replies": [],
          "text": "4. Reconnect afterwards. \nOnce the consequence is over, give your child a chance to do something good, and praise them for it.",
          "type": "send_msg",
          "uuid": "cc8d6ae9-d5ef-4183-8cd7-69558b8ffa6d"
        }
      ],
      "exits": [
